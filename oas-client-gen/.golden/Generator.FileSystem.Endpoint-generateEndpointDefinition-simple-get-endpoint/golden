getHealth :: Endpoint () () Text
getHealth = Endpoint
  { method = HTTP.GET
  , path = const $ fold
      [ "/health" ]
  , toRequestBody = const L.empty
  , fromResponseBody = \case
      200 -> \bs -> first (ParseError 200 . L8.pack) $ A.eitherDecode bs
      s -> \bs -> Left $ UnexpectedResponse s bs

  }
