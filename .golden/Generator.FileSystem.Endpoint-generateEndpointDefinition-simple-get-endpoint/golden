getHealth :: Endpoint () () Text
getHealth = Endpoint
  { method = GET
  , path = const $ fold
      [ "/health" ]
  , requestBody = const L.empty
  , responseBody = \case
      ForStatus 200 -> \bs -> first (StatusCaseParseError 200) bs $ A.eitherDecode bs
      s -> Left . UnexpectedResponse s

  }
