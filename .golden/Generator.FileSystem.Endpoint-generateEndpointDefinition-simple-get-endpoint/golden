getHealth :: Endpoint () () Text
getHealth = Endpoint
  { method = GET
  , path = const $ fold
      [ "/health" ]
  , requestBody = const L.empty
  , responseBody = \case
      ForStatus 200 -> s -> first StatusCaseParseError ForStatus 200 bs $ A.decodeEither bs
      s -> Left . UnexpectedResponse s

  }
